{
    "name": "nunes/rumobr-theme",
    "description": "Tema padr√£o para ser utilizado em todos os projetos interno e externos da Rumobr Tecnologia (www.rumobr.com.br)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "vue",
        "components",
        "theme",
        "ui"
    ],
    "authors": [
        {
            "name": "Leandro Nunes",
            "email": "leandro.nunes@rumobr.com",
            "homepage": "https://github.com/leandrorumobr"
        }
    ],
    "homepage": "https://github.com/leandrorumobr/rumobr-theme",
    "support": {
        "issues": "https://github.com/leandrorumobr/rumobr-theme/issues",
        "source": "https://github.com/leandrorumobr/rumobr-theme"
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nunes\\RumobrTheme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nunes\\RumobrTheme\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nunes\\RumobrTheme\\RumobrThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    }
}